{
  "scripts": {
    "format": "prettier --write **/*.js **/*.json",
    "dev:chin": "chin development -v",
    "dev:dll": "webpack --config ./webpack.config.dll.babel.js",
    "dev:pre": "run-s dev:chin dev:dll",
    "dev": "webpack-dev-server --config ./webpack.config.dev.babel.js",
    "pro:chin": "chin production -v",
    "pro:pack": "webpack -p --config ./webpack.config.pro.babel.js",
    "pro": "run-s pro:chin pro:pack",
    "pro:serve": "http-server .site -a localhost -p 7000 -o -c-1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          }
        }
      ],
      "flow",
      "react"
    ],
    "plugins": []
  },
  "dependencies": {
    "atra": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "orph": "^1.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chin": "^2.1.0",
    "favicons": "^4.8.6",
    "flow-bin": "^0.58.0",
    "flow-typed": "^2.2.1",
    "fs-extra": "^4.0.2",
    "http-server": "^0.10.0",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.7.4",
    "pug": "^2.0.0-rc.2",
    "rehype": "^5.0.0",
    "rehype-format": "^2.2.0",
    "rehype-react": "^3.0.1",
    "remark": "^8.0.0",
    "remark-rehype": "^2.0.1",
    "unified": "^6.1.5",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.0"
  }
}
